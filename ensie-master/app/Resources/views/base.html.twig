    <!-- Styles -->
    {% block stylesheets %}
        <link rel="stylesheet" href="/css/master.css" />
        <link rel="stylesheet" href="{{ asset('bundles/ensiesimpletext/css/glyphicon.css') }}" />
    {% endblock %}
    <!-- Style block allowing child pages to inject their own styles -->

    {% block socialmedia_tags %}
    {% endblock %}

    <!-- include JS libs -->
    {% block javascripts %}
        {% javascripts
        '@EnsieAssetBundle/Resources/public/js/jquery.min.js'
        '@EnsieAssetBundle/Resources/public/js/jquery-ui.min.js'
        '@EnsieAssetBundle/Resources/public/js/modernizr.min.js'
        '@EnsieAssetBundle/Resources/public/js/responsiveslides.min.js'
        '@EnsieAssetBundle/Resources/public/js/url.min.js'

        %}
