        <div class="block third left">
            <h2>{{ simpleText('footer.popular_writers') }}{{ simpleTextSonataAdminLink('footer.popular_writers') | raw }}</h2>
            {{ render(controller('EnsieUserBundle:User:popularUserList', {'maxResult': 5})) }}
