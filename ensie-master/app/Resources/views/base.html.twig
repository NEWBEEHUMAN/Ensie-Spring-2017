        {% endjavascripts %}
    {% endblock %}

    <!-- Typekit integration -->
    <script type="text/javascript" src="https://use.typekit.net/ivv4coi.js"></script>
    <script type="text/javascript">try{Typekit.load();}catch(e){}</script>

    {% block javascripts2 %}
        {% javascripts
        '@EnsieAssetBundle/Resources/public/js/common.js'
        %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}
</head>
<body>
{% block header %}
    <div class="notifications">
        {% for flashMessage in app.session.flashbag.get('succes') %}
            <div class="notification success" data-type="success">
                <p>{{ simpleText(flashMessage, translationPlaceHolders)|raw }}{{ simpleTextSonataAdminLink(flashMessage)|raw }}</p>
                <span class="close"></span>
            </div>
        {% endfor %}

        {% for flashMessage in app.session.flashbag.get('error') %}
            <div class="notification error" data-type="error">
                <p>{{ simpleText(flashMessage, translationPlaceHolders)|raw }}{{ simpleTextSonataAdminLink(flashMessage)|raw }}</p>
                <span class="close"></span>
            </div>
        {% endfor %}

        {% for flashMessage in app.session.flashbag.get('notice') %}
            <div class="notification normal" data-type="normal">
                <p>{{ simpleText(flashMessage, translationPlaceHolders)|raw }}{{ simpleTextSonataAdminLink(flashMessage)|raw }}</p>
                <span class="close"></span>
            </div>
        {% endfor %}
    </div>
{% if app.request.get('_route') == 'home' %}
<header class="pageheader">
{% else %}
<header class="pageheader sub">
