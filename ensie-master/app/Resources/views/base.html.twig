<div class="overlay">
    {% block overlays %}
        <div class="modal" data-selector="login">
            <header>
                <h2>{{ simpleText('overlay.login_title') }}{{ simpleTextSonataAdminLink('overlay.login_title') | raw }}</h2>
            </header>
            <div class="modal-content">
                <p>{{ simpleText('overlay.login_description') }}{{ simpleTextSonataAdminLink('overlay.login_description') | raw }}</p>
                {% render url('fos_user_security_login') %}
            </div>
        </div>
        <div class="modal" data-selector="resetting">
            <header>
                <h2>{{ simpleText('overlay.resetting_title') }}{{ simpleTextSonataAdminLink('overlay.resetting_title') | raw }}</h2>
            </header>
            <div class="modal-content">
                {% render url('fos_user_resetting_request') %}
            </div>
        </div>
        <div class="modal" data-selector="register">
            <header>
                <h2>{{ simpleText('overlay.login_register') }}{{ simpleTextSonataAdminLink('overlay.login_register') | raw }}</h2>
            </header>
            <div class="modal-content">
                <p>{{ simpleText('overlay.registration_description') }}{{ simpleTextSonataAdminLink('overlay.registration_description') | raw }}</p>
                <form action="{{ hwi_oauth_login_url('linkedin') }}">
                    <ul class="form">
                        <li class="buttonwrapper">
                            <input type="submit"
                                   value="{{ simpleText('overlay.registration_register_linkedin_button_value') }}"/>

                            <div class="spinner"></div>
                            {{ simpleTextSonataAdminLink('overlay.registration_register_linkedin_button_value') | raw }}
                        </li>
                        <li class="buttonwrapper">
                            <input type="button" onclick="window.location.href='{{ url('register_company') }}'"
                                   value="{{ simpleText('overlay.registration_register_company_button_value') }}"/>

                            <div class="spinner"></div>
                            {{ simpleTextSonataAdminLink('overlay.registration_register_company_button_value') | raw }}
                        </li>
                        <li class="center">
                            <a href="#"
                               data-action="login">{{ simpleText('overlay.register_already_account') }}</a>{{ simpleTextSonataAdminLink('overlay.register_already_account') | raw }}
                        </li>
                    </ul>
                </form>
            </div>
        </div>
        <div class="modal" data-selector="register-company">
            <header>
                <h2>{{ simpleText('overlay.login_register_company') }}{{ simpleTextSonataAdminLink('overlay.login_register_company') | raw }}</h2>
            </header>
            <div class="modal-content">
                {% render url('fos_user_registration_register') %}
            </div>
        </div>
        <div class="modal" data-selector="delete">
            <header>
                <h2>{{ simpleText('profile.definition_delete_overlay_title_%word%') }}</h2>{{ simpleTextSonataAdminLink('profile.definition_delete_overlay_title_%word%')|raw }}
            </header>
            <div class="modal-content">
                <p>{{ simpleText('profile.definition_delete_overlay_content_%word%') }}</p>{{ simpleTextSonataAdminLink('profile.definition_delete_overlay_content_%word%')|raw }}
                <ul class="form">
                    <li class="buttonwrapper">
                        <input type="submit" value="{{ simpleText('profile.definition_delete_overlay_button_value') }}"
                               class="delete">
                        <input type="submit" value="{{ simpleText('profile.definition_delete_overlay_button_cancel') }}"
                               class="cancel">{{ simpleTextSonataAdminLink('profile.definition_delete_overlay_button_cancel')|raw }}
                    </li>
                    <li class="center">
                        <a href="">{{ simpleText('profile.definition_delete_overlay_button_cancel') }}</a>{{ simpleTextSonataAdminLink('profile.definition_delete_overlay_button_cancel')|raw }}
                    </li>
                </ul>
            </div>
        </div>
        {% if token is defined and token %}
            <div class="modal" data-selector="reset">
                <header>
                    <h2>{{ simpleText('overlay.reset_title') }}{{ simpleTextSonataAdminLink('overlay.reset_title') | raw }}</h2>
                </header>
                <div class="modal-content">
                    {% render url('fos_user_resetting_reset', {'token': token}) %}
                    <input type="hidden" data-action="reset">
                </div>
            </div>
        {% endif %}
    {% endblock %}
</div>

<!-- Script block allowing child pages to inject their own scripts -->
{% block childJavascripts %}
    <script type="text/javascript">
        var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
        document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
    </script>
    <script type="text/javascript">
        try {
            if(location.hostname.match('ensie.nl')) {
                var pageTracker = _gat._getTracker("UA-296946-15");
                console.log('detected .nl');
            } else {
                (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
                })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

                ga('create', 'UA-296946-18', 'auto');
                ga('send', 'pageview');
                console.log('detected .com');
            }
            pageTracker._trackPageview();
        } catch(err) {}
    </script>
    {% javascripts '@EnsieAssetBundle/Resources/public/js/app.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {% if login is defined and login is not null and login and app.user == null %}
    <script type="text/javascript">
        $(function() {
            $('[data-action="login"]').click();
        });
    </script>
    {% endif %}
    {% if token is defined and token is not null and token %}
    <script type="text/javascript">
        $(function() {
            $('[data-action="reset"]').click();
        });
    </script>
    {% endif %}
    {% if app.user != null %}
    <script type="text/javascript">
        $(document).ready(function() {

            $('[data-action="delete"]').click(function(){
                dataSelector = $('[data-selector="delete"]');
                var slug = $(this).data('id');
                var word = $(this).data('word');
                var url = $(this).data('url');
                var elem = dataSelector.find("header h2");
                elem.text(elem.text().replace("%word%", word));
                elem = dataSelector.find(".modal-content p");
                elem.html(elem.html().replace("%word%", '<strong>' + word + '</strong>'));
                $('.modal-content .form .delete').click(function(event){
                    event.preventDefault()
                    window.location = url;
                });
                $('.modal-content .form .cancel').click(function(event){
                    $('.overlay:visible').fadeOut('fast', function() { $('[data-selector="delete"]').fadeOut('fast'); });
                });
            });

            {#{% if ensieUser.firstLogin %}#}
            {#$('.overlay:hidden').fadeIn('fast', function() { $(this).find('[data-selector="profile-edit"]').fadeIn('fast'); });#}
            {#{% endif %}#}
        });
    </script>
    {% endif %}
{% endblock %}

{% block metadata_edit %}
    {{ simpleTextSonataAdminLink('metadata.title') | raw }}
    {{ simpleTextSonataAdminLink('metadata.description') | raw }}
    {{ simpleTextSonataAdminLink('metadata.keywords') | raw }}
{% endblock %}
</body>
