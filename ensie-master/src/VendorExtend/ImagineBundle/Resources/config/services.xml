<?xml version="1.0" encoding="utf-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="liip_imagine.controller" class="VendorExtend\ImagineBundle\Controller\ImagineExtendedController">
            <argument type="service" id="liip_imagine.data.manager" />
            <argument type="service" id="liip_imagine.filter.manager" />
            <argument type="service" id="liip_imagine.cache.manager" />
            <argument type="service" id="liip_imagine.cache.signer" />
            <call method="setFilterConfiguration">
                <argument type="service" id="liip_imagine.filter.configuration" />
            </call>
        </service>
    </services>
</container>
