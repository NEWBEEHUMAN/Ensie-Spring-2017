                            {#https://www.linkedin.com/shareArticle?summary=Article+%word%+written+by+%author%&title=%word%&mini=true&url=%url%&source=Your+source                            #}
                            <a href="{{ simpleText('definition.share_link_linked_in_%word%_%author%_%host%', {'word': definition.word, 'author': definition.ensieUser.formattedName, 'url': shareLink }) }}"
                               target="_blank">LinkedIn</a>
                        </div>
                        {% if not app.user %}
