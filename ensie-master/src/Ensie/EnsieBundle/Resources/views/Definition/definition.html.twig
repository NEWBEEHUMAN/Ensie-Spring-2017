{% extends "base.html.twig" %}

{% block metadata %}
    {% set ensieUser = definition.ensieUser %}
    {% if app.request.locale == 'nl' %}
        {% set page = 'search' %}
        <title>{{ definition.word }} | betekenis en definitie
            door {% if ensieUser.isCompany %}{{ ensieUser.companyName }}{% else %}{{ ensieUser.formattedName }}{% endif %}</title>
        <meta name="description"
              content="Wat betekent {{ definition.word }}? Hier vind je de definitie en betekenis van {{ definition.word }}">
        <meta name="keywords"
              content="wat betekent, definitie, betekenis, {{ definition.word }}, wat is, expert, kennis, informatie"/>
    {% else %}
        {% set page = 'search' %}
        <title>{{ definition.word }} | meaning and definition
            by {% if ensieUser.isCompany %}{{ ensieUser.companyName }}{% else %}{{ ensieUser.formattedName }}{% endif %}</title>
        <meta name="description"
              content="What means {{ definition.word }}? Here you find the definition and meaning of {{ definition.word }}">
        <meta name="keywords"
              content="what means, definition, meaning, {{ definition.word }}, what is, expert, knowledge"/>
    {% endif %}
{% endblock %}

{% block socialmedia_tags %}
    {% set ensieUser = definition.ensieUser %}
    <!-- Schema.org markup for Google+ -->
    <meta itemprop="name"
          content="{{ definition.word }} | betekenis en definitie door {% if ensieUser.isCompany %}{{ ensieUser.companyName }}{% else %}{{ ensieUser.formattedName }}{% endif %}">
    <meta itemprop="description"
          content="Wat betekent {{ definition.word }}? Hier vind je de definitie en betekenis van {{ definition.word }}">
    {% if app.request.locale == 'nl' %}
        <meta itemprop="image" content="https://www.ensie.nl/img/logo.png">
    {% else %}
        <meta itemprop="image" content="https://www.ensie.com/img/logo.png">
    {% endif %}

    <!-- Open Graph data -->
    <meta property="og:title"
          content="{{ definition.word }} | betekenis en definitie door {% if ensieUser.isCompany %}{{ ensieUser.companyName }}{% else %}{{ ensieUser.formattedName }}{% endif %}"/>
    <meta property="og:type" content="article"/>
    <meta property="og:url" content="{{ definitionPath(definition) }}"/>
    {% if app.request.locale == 'nl' %}
        <meta property="og:image" content="http://www.ensie.nl/img/logo.png"/>
        <meta property="og:description"
              content="Wat betekent {{ definition.word }}? Hier vind je de definitie en betekenis van {{ definition.word }}"/>
    {% else %}
        <meta property="og:image" content="http://www.ensie.com/img/logo.png"/>
        <meta property="og:description"
              content="What means {{ definition.word }}? Here you find the definition and meaning of {{ definition.word }}"/>
    {% endif %}

{% endblock %}

{% block content %}
    {% if definition.ensieUser.isCompany and definition.ensieUser.headerImage %}
    <section class="businessoverviewpage">
        <header class="businessoverview-header" style="background-image: url({{ getCompanyHeaderImage(definition.ensieUser, 'header_max') }})">
            <div class="business-info"></div>
        </header>
        {% if definition.extraLinkUrl %}
        <a href="{{ definition.extraLinkUrl }}" target="_blank" class="button cta">{% if definition.extraLinkText %}{{ definition.extraLinkText }}{% else %}Ga naar de website{% endif %}</a>
        {% endif %}
    </section>
    {% endif %}
    <article class="resultdetail {% if definition.ensieUser.isCompany and definition.ensieUser.headerImage %}businessdetail{% endif %}">
        <div class="wrapper">
            <section class="content">
                <header>
                    <h1>
                        {{ definition.word }}
                        <span>{{ simpleText('searchpage.definition_and_meaning') }}{{ simpleTextSonataAdminLink('searchpage.definition_and_meaning') | raw }}</span>
                    </h1>

                    <div class="rating">
                        <div class="stars" data-rating="{{ definition.ratingCount|round(1) }}">
                            {% for i in 1..5 %}
                                {% if i < 3 %}
                                    <input type="radio" value="{{ i }}" name="rating" class="feedback"
                                           data-url="{{ path('definition_definition_rating', { 'userSlug': definition.ensieUser.slug, 'definitionSlug': definition.slug, 'ratingValue': i}) }}">
                                {% else %}
                                    <input type="radio" value="{{ i }}" name="rating"
                                           onclick="window.location = '{{ path('definition_definition_rating', { 'userSlug': definition.ensieUser.slug, 'definitionSlug': definition.slug, 'ratingValue': i}) }}';">
                                {% endif %}
                            {% endfor %}
                            <span></span>
                        </div>
                    </div>
                </header>
                <div class="text">
                    <p class="definition">{{ definition.definition|nl2br }}</p>
                    <p class="meaning">{{ definition.description|nl2br }}</p>
                </div>
                <footer>
					<span class="updated">
						{{ simpleText('definition.last_updated') }}{{ simpleTextSonataAdminLink('definition.last_updated') | raw }}
                        : {{ definition.lastTextUpdate|date('d-m-Y') }}
					</span>

                    <div class="buttons">
                        <a class="share" data-show="share-box" href="#">
