                    {% endblock %}
                    {% block profilewrapper_favorite %}
                    {% endblock %}
                {% endembed %}

                {% if ensieDefinitionList is not empty %}
                    <h4>Meer uit Ensie {{ ensieDefinitionList[0].ensie.title }}</h4>
                    <ul class="other">
                        {% for ensieDefinition in ensieDefinitionList %}
                            <li><a href="{{ definitionPath(ensieDefinition) }}">{{ ensieDefinition.word }}</a></li>
                        {% endfor %}
                    </ul>
                {% endif %}

                {% if definition.ensieUser.slug == 'redactie-ensie' %}
                {% endif %}

            </aside>
        </div>
    </article>
{% endblock %}

