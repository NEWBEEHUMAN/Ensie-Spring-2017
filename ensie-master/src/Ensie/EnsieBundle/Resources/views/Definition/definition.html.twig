                <ul class="form">
                    <li>
                        <label for="i1">{{ simpleText('overlay.low_rating_reason') }}{{ simpleTextSonataAdminLink('overlay.low_rating_reason') | raw }}</label>
                        <textarea id="i1" name="feedback" required></textarea>
                    </li>
