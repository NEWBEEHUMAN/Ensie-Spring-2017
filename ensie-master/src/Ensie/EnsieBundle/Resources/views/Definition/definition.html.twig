    <div class="modal" data-selector="spam">
        <header>
            <h2>{{ simpleText('overlay.spam_report_title') }}{{ simpleTextSonataAdminLink('overlay.spam_report_title') | raw }}</h2>
        </header>
        <div class="modal-content">
            {{ simpleText('overlay.spam_report_text') | raw }}{{ simpleTextSonataAdminLink('overlay.spam_report_text', true) | raw }}
            <form method="post"
                  action="{{ path('definition_definition_report_spam', { 'userSlug': definition.ensieUser.slug, 'definitionSlug': definition.slug}) }}">
                <ul class="form">
                    <li>
                        <label for="i1">{{ simpleText('overlay.spam_report_reason') }}{{ simpleTextSonataAdminLink('overlay.spam_report_reason') | raw }}</label><textarea
                                id="i1" name="description"></textarea></li>
                    <li class="buttonwrapper">
                        <input type="button" value="{{ simpleText('overlay.spam_report_button') }}"
                               onclick="form.submit();">{{ simpleTextSonataAdminLink('overlay.spam_report_button') | raw }}
                    </li>
                </ul>
            </form>
        </div>
    </div>
    <div class="modal" data-selector="low-rating">
        <header>
            <h2>{{ simpleText('overlay.low_rating_title') }}{{ simpleTextSonataAdminLink('overlay.low_rating_title') | raw }}</h2>
        </header>
        <div class="modal-content">
            {{ simpleText('overlay.low_rating_text') | raw }}{{ simpleTextSonataAdminLink('overlay.low_rating_text', true) | raw }}
