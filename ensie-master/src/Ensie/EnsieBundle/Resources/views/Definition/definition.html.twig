                                    <path d="M1.8 49.9c-1 0-1.8-0.8-1.8-1.9V1.7c0-0.3 0.1-0.5 0.2-0.8l0.1-0.2c0.1-0.1 0.2-0.2 0.3-0.3l0.1-0.1L1.1 0C1.2 0 1.3-0.1 1.5-0.1c0 0 0 0 0.1 0l0.3 0c0.3 0 0.5 0.1 0.7 0.2l29.3 12.8c0.7 0.3 1.1 1 1.1 1.7 0 0.7-0.4 1.4-1.1 1.7L3.7 29.3v18.8C3.7 49.1 2.8 49.9 1.8 49.9zM3.7 4.5v20.7l22.9-10.6L3.7 4.5z"/></svg>
                            </a>
                        {% endif %}
                    </div>
                </footer>
            </section>

            <aside class="profile">
                {% embed 'EnsieUserBundle:User:profileWrapper.html.twig' with { 'ensieUser': definition.ensieUser } %}
                    {% block overlays_extra %}
                    {% endblock %}
                    {% block socialprofiles %}
                        {% if app.user == definition.ensieUser %}
                            <div class="socialprofiles">
                                <a class="edit"
                                   href="{{ path('profile_definition_edit', {'slug': definition.slug }) }}">
                                    <svg xml:space="preserve" enable-background="new 0 0 59.1 59.1"
